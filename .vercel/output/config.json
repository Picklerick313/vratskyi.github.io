{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/en\\/404\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/en\\/about\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/en\\/blog\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/en\\/portfolio\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/en\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/ru\\/404\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/ru\\/about\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/ru\\/blog\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/ru\\/portfolio\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/ru\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/$",
			"dest": "/_isr?x_astro_path=$0"
		}
	]
}